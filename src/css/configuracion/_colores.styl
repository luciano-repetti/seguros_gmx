gray-1-hex		= #FDFDFD

blue-1-hex		= #F7F8FE
blue-2-hex		= #D7EAEF
blue-3-hex		= #B8D1D6
blue-4-hex		= #96BAC2

blueDark-1-hex 	= #005581
