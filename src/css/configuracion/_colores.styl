	--white			: white-hex

	--gray-1		: gray-1-hex

	--blue-1		: blue-1-hex
	--blue-2		: blue-2-hex
	--blue-3		: blue-3-hex
	--blue-4		: blue-4-hex
	
	--blueDark-1	: blueDark-1-hex
	--blueDark-2	: blueDark-2-hex
	--blueDark-3	: blueDark-3-hex
	--blueDark-4	: blueDark-4-hex
