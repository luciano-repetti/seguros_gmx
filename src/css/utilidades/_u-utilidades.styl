.u-hidden
	display none

.u-visible
	display block

.u-float-left
	float left

.u-float-right
	float right

.u-clear-both
	clear both

.u-text-center
	text-align center

.u-text-left
	text-align left

.u-text-right
	text-align right

//Screen read only accesibility
.sr-only:not(:focus):not(:active) 
	clip rect(0 0 0 0)
	clip-path inset(100%)
	height 1px
	overflow hidden
	position absolute
	white-space nowrap
	width 1px


.u-title
	clip rect(1px, 1px, 1px, 1px)
	position absolute
