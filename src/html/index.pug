						.c-tabs__panel#tabpanel-3.js-tabsPanel(role='tabpanel' tabindex='0' aria-labelledby='tab-3' data-title='Personales')
							- var personales = [{'name': 'Hogar','img': 'GMX-Hogar.png','link': '#!'},{'name': 'Mascotas','img': 'GMX-Mascotas.png','link': '#!'},];
							a.c-tabs__link.js-tabsLink(href="#" role='button' aria-expanded="false" data-id="tab-3") Personales
							.c-tabs__content.js-tabsContent
