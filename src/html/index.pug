							.c-tabs__content.js-tabsContent
								ul.o-list
									li
										a.o-card-product(href="#" title="Rc Profesional")
											
											img.o-card-product__img.js-lazy(src=urlFolder+"images/global/GMX-Placeholder-Small.jpg" data-src=urlFolder+"images/home/GMX-Profesional.png" alt=" Imagen de Rc Profesional")
											strong Rc Profesional
									li
										a.o-card-product(href="#" title="Odontólogos")
											
											img.o-card-product__img.js-lazy(src=urlFolder+"images/global/GMX-Placeholder-Small.jpg" data-src=urlFolder+"images/home/GMX-Odontologos.png" alt=" Imagen de Odontólogos")
											strong Odontólogos
									li
										a.o-card-product(href="#" title="Médico Veterinario")
											
											img.o-card-product__img.js-lazy(src=urlFolder+"images/global/GMX-Placeholder-Small.jpg" data-src=urlFolder+"images/home/GMX-Veterinario.png" alt=" Imagen de Médico Veterinario")
											strong Médico Veterinario
									li
										a.o-card-product(href="#" title="Servidores Públicos")
											
											img.o-card-product__img.js-lazy(src=urlFolder+"images/global/GMX-Placeholder-Small.jpg" data-src=urlFolder+"images/home/GMX-Servidores-Publicos.png" alt=" Imagen de Servidores Públicos")
											strong Servidores Públicos
									li
										a.o-card-product(href="#" title="Médicos y Profesionales de la Salud")
											
											img.o-card-product__img.js-lazy(src=urlFolder+"images/global/GMX-Placeholder-Small.jpg" data-src=urlFolder+"images/home/GMX-Medicos.png" alt=" Imagen de Médicos y Profesionales de la Salud")
											strong Médicos y Profesionales de la Salud

						.c-tabs__panel#tabpanel-2.js-tabsPanel(role='tabpanel' tabindex='0' aria-labelledby='tab-2' data-title='Empresariales')
