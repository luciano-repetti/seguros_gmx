					li
						.c-card-type1.c-card-type1--styl4.o-card
							.c-card-type1__image
								img.js-lazy(src=urlFolder+"images/global/GMX-Placeholder-Medium.jpg" data-src=urlFolder+"images/home/GMX-Seguros.jpg" alt="Imagen GMX Seguros")
