									each personal in personales
										li
											a.o-card-product(href=personal.link title=personal.name)
												
												img.o-card-product__img.js-lazy(src=urlFolder+"images/global/GMX-Placeholder-Small.jpg" data-src=urlFolder+"images/home/"+personal.img alt=" Imagen de"+ personal.name)
												strong= personal.name
							
						.c-tabs__panel#tabpanel-4.js-tabsPanel(role='tabpanel' tabindex='0' aria-labelledby='tab-4' data-title='Construcción')
							- var construcciones = [{'name': 'RC Construcción','img': 'GMX-Construccion.png','link': '#!'},{'name': 'Decenal','img': 'GMX-Decenal.png','link': '#!'},];
