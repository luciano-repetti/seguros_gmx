extends template/_template

append configuracion
	-var page			= {}
	-page['link']		= 'index.html'
	-page['title']		= 'Inicio'
	-page['descrip']	= 'description'
	-page['keys']		= 'keywords'
	-page['lang']		= 'es'
	-page['responsive']	= true
	-page['section']	= []
	-page['libsCss']	= []
	-page['libs']		= ['_alert', '_customForm', '_fillViews', '_select', '_validate']
	-page['plugins']	= ['splide']
	-page['js']			= [] 

	- var path = ''
		if(page.link !== 'index.html')
			- path = '../'

block contenido
	main.o-section--main
		section.c-banner-text
			//h1.u-title GMX Seguros
			.c-banner-text__box
				.o-container
					.c-banner-text__container
						.c-banner-text__title
							h1 Construimos un Futuro #[strong Seguro.]
						.c-banner-text__text 
							p Descubre la tranquilidad en cada aspecto de tu vida. Contamos con seguros diseñados a la medida.
							p Tu futuro más seguro comienza aquí.
		section.o-section--top
			.o-container
				.c-tabs
					h2.u-title#tablist-1 Seguros para tu negocio
					.c-tabs__nav.js-tabs
						ul.js-tabsNav(role='tablist' aria-labelledby='tablist-1')
							li.is-active(role="presentation")
								a.js-tabsBtn#tab-1(href="#" title="Mostrar Profesionistas" role='tab' aria-selected='true' aria-controls='tabpanel-1') Profesionistas
							li(role="presentation")
								a.js-tabsBtn#tab-2(href="#" title="Mostrar Empresariales" role='tab' aria-selected='false' aria-controls='tabpanel-2' tabindex='-1') Empresariales
							li(role="presentation")
								a.js-tabsBtn#tab-3(href="#" title="Mostrar Personales" role='tab' aria-selected='false' aria-controls='tabpanel-3' tabindex='-1') Personales
							li(role="presentation")
								a.js-tabsBtn#tab-4(href="#" title="Mostrar Construcción" role='tab' aria-selected='false' aria-controls='tabpanel-4' tabindex='-1') Construcción
							li(role="presentation")
								a.js-tabsBtn#tab-5(href="#" title="Mostrar Transportes y Aviación" role='tab' aria-selected='false' aria-controls='tabpanel-5' tabindex='-1') Transportes y Aviación
							li(role="presentation")
								a.js-tabsBtn#tab-6(href="#" title="Mostrar Sector Salud" role='tab' aria-selected='false' aria-controls='tabpanel-6' tabindex='-1') Sector Salud
							li(role="presentation")
								a.js-tabsBtn#tab-7(href="#" title="Mostrar Hotelería y Recreación" role='tab' aria-selected='false' aria-controls='tabpanel-7' tabindex='-1') Hotelería y Recreación
