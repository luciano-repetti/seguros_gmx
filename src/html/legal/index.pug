extends ../template/_template

block append configuracion
	- var page = {};
	- page['link'] = 'legal/index.html';
	- page['title'] = 'Legal';
	- page['descrip'] = 'Resultados financieros';
	- page['keys'] = 'seguros, negocios, personales';
	- page['lang'] = 'es';
	- page['responsive'] = true;
	- page['section'] = [];
	- page['libsCss'] = [];
	- page['libs'] = [];
	- page['plugins'] = ['splide'];
	- page['js'] = [];

	- var path = '../../';

