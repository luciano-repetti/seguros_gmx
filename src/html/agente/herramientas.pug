extends ../template/_template

block append configuracion
	- var page = {};
	- page['link'] = 'agente/herramientas.html';
	- page['title'] = 'Soy agente';
	- page['descrip'] = 'Herramientas';
	- page['keys'] = 'seguros, negocios, personales';
	- page['lang'] = 'es';
	- page['responsive'] = true;
	- page['section'] = [];
	- page['libsCss'] = [];
	- page['libs'] = [];
	- page['plugins'] = ['splide'];
	- page['js'] = [];

	- var path = '../../';

block contenido
	main.o-section--main
		.c-banner-text.c-banner-text--padding1
			section.o-section.o-section--banner-small
				.o-container
					.o-module.o-module--deco5.u-text-center
						.o-module__title.o-module__title--column#title
							h1 #[small Soy agente] Herramientas
		.o-section
			.o-section.o-section--max-1000
				.c-card-type4__container
					a(href="#" title="Documentos emitidos").c-card-type4__card
						img.js-lazy(
							src=urlFolder + 'images/global/GMX-Placeholder-Small.jpg'
							data-src=urlFolder + 'images/soy-agente/GMX-documentos-emitidos.svg' alt='Documentos emitidos')
						p Documentos emitidos
						.o-btn-arrow.o-btn-arrow--blue
							i.i-icon.i-arrow-up-right(aria-hidden='true')
					a(href="#" title="Actualización de datos fiscales").c-card-type4__card
