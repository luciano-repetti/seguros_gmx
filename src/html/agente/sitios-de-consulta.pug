extends ../template/_template

block append configuracion
	- var page = {};
	- page['link'] = 'agente/sitios-de-consulta.html';
	- page['title'] = 'Soy agente';
	- page['descrip'] = 'Sitios de consulta';
	- page['keys'] = 'seguros, negocios, personales';
	- page['lang'] = 'es';
	- page['responsive'] = true;
	- page['section'] = [];
	- page['libsCss'] = [];
	- page['libs'] = [];
	- page['plugins'] = ['splide'];
	- page['js'] = [];

	- var path = '../../';

