				.o-container
					.o-module.o-module--deco5.u-text-center
						.o-module__title.o-module__title--column#title
							h1 #[small Soy agente] Sitios de #[strong Consulta]
		.o-section
			.o-section.o-section--max-800
				.c-consultation__container
